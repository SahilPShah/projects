module bullet_fsm(input Clk, RESET
						input logic [9:0] Megaman_x_position, Megaman_y_position, bullet1X, bullet2X bullet3X, bullet1Y, bullet2Y, bullet3 Y
						input frame_clk
						input direction
						input bullet1_on, bullet2_on, bullet3_on);
						

logic frame_clk_delayed, frame_clk_rising_edge;
always_ff @ (posedge Clk)
	begin
		frame_clk_delayed <= frame_clk;
      frame_clk_rising_edge <= (frame_clk == 1'b1) && (frame_clk_delayed == 1'b0);
	end

